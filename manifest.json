  {
    "name": "Saved Post Organizer Extension",
    "description" : "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "SavedPostOrganizer.html",
        "default_icon": "images/icons8-checkmark-26.png"
      },
      "web_accessible_resources": [
        "SavedPostOrganizer.js",
        "SavedPostOrganizer.html"
      ],
      "permissions": [
        "https://ssl.reddit.com/",
        "https://oauth.reddit.com/",
        "https://www.reddit.com/*",
        "activeTab"
      ],
      "background": {
        "persistent": false,
        "scripts": ["SavedPostOrganizer.js"]
      }
  }